<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/container_lista_clientes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <androidx.cardview.widget.CardView
            android:id="@+id/cardview_pendentes"
            android:layout_width="0dp"
            android:layout_height="150dp"
            android:layout_marginTop="10dp"
            android:layout_marginHorizontal="10dp"
            app:cardBackgroundColor="@color/BotaoPrincipal"
            app:layout_constraintStart_toStartOf="@id/container_lista_clientes"
            app:layout_constraintEnd_toEndOf="@id/container_lista_clientes"
            app:layout_constraintTop_toTopOf="@id/container_lista_clientes"
            app:layout_constraintBottom_toBottomOf="@id/container_lista_clientes"
            >



        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>